
#Maquina em que esta o barramento.
HostBus='localhost'

#Porta da maquina em que esta o barramento.
PortBus='2089'

#Nome do usuario Openbus a ser utilizado pelo processo cliente.
BusUser='tester'

#Senha do usuario Openbus a ser utilizada pelo processo cliente.
BusPassword='tester'

#Nome da entidade a ser autenticada através de um certificado digital.
EntityName='HelloService'

#Nome do arquivo referente a chave privada da entidade.
PrivateKeyFilename='HelloService.key'

#Nome do arquivo referente ao certificado do ACS.
ACSCertificateFilename='AccessControlService.crt'

#Nome da faceta referente ao servico Hello.
FacetName='IHello'
